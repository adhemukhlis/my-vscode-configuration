{
  "editor.autoClosingQuotes": "always",
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorWidth": 4,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "'Dank Mono', 'monospace'",
  "editor.fontLigatures": true,
  "editor.fontSize": 18,
  "editor.fontWeight": 600,
  "editor.lineHeight": 36,
  "editor.minimap.side": "left",
  "editor.mouseWheelZoom": true,
  "editor.linkedEditing": true,
  "editor.renderFinalNewline": "off",
  "editor.smoothScrolling": true,
  "editor.stablePeek": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.hover.delay": 1200,
  "editor.letterSpacing": 0.8,
  "editor.tabSize": 2,
  "editor.indentSize": "tabSize",
  "editor.renderControlCharacters": false,
  "editor.accessibilitySupport": "off",
  "editor.lightbulb.enabled": false,
  "editor.guides.bracketPairs": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.renderWhitespace": "boundary",
  "editor.codeActionWidget.includeNearbyQuickFixes": false,
  "editor.codeActionWidget.showHeaders": false,
  "editor.colorDecoratorsActivatedOn": "click",
  "editor.scrollbar.verticalScrollbarSize": 8,
  "editor.snippetSuggestions": "top",
  "files.trimTrailingWhitespace": true,
  "window.autoDetectHighContrast": false,
  "window.titleSeparator": " ðŸ™ˆ ",
  "telemetry.telemetryLevel": "off",
  "workbench.tips.enabled": false,
  "workbench.layoutControl.enabled": false,
  "files.autoSave": "onFocusChange",
  "window.menuBarVisibility": "toggle",
  "window.title": "${dirty}${activeEditorShort}${separator}${rootName}",
  "window.titleBarStyle": "custom",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.scrollToSwitchTabs": true,
  "workbench.fontAliasing": "antialiased",
  "workbench.list.smoothScrolling": true,
  "workbench.editor.enablePreview": false,
  "diffEditor.codeLens": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "notebook.outline.showCodeCells": true,
  "gitlens.defaultDateFormat": "DD MMM, YYYY",
  "security.workspace.trust.banner": "always",
  "workbench.tree.indent": 16,
  "workbench.editor.tabCloseButton": "left",
  "git.openRepositoryInParentFolders": "never",
  "workbench.startupEditor": "none",
  "workbench.colorTheme": "Palenight Theme",
  "workbench.iconTheme": "material-icon-theme",
  "editor.quickSuggestions": {
    "comments": false,
    "other": true,
    "strings": true
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "entity.name.type.class",
          "keyword",
          "constant",
          "storage.modifier",
          "storage.type.class.js"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "invalid",
          "keyword.operator",
          "constant.numeric.css",
          "keyword.other.unit.px.css",
          "constant.numeric.decimal.js",
          "constant.numeric.json"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "files.exclude": {
    "**.svg": true,
    "**/build": true,
    "**/dist": true,
    "**/node_modules": true,
    "**/package-lock.json": true,
    "**/yarn.lock": true
  },
  "files.watcherExclude": {
    "**/build/**": true
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "search.exclude": {
    "**/build": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.userWords": [
    "Antd",
    "bulan",
    "COOLDOWN",
    "daftar",
    "Kuota",
    "Rekening",
    "tahun",
    "vuex"
  ],
  "diffEditor.experimental.useVersion2": true,
  "compareFolders.ignoreEmptyLines": true,
  "compareFolders.excludeFilter": [
    "**/node_modules",
    "**/dist",
    "**/.next",
    "**/.svn",
    "**/.git",
    "**/package-lock.json"
  ],
  "todo-tree.highlights.defaultHighlight": {
    "icon": "alert",
    "type": "text",
    "foreground": "red",
    "background": "white",
    "opacity": 50,
    "iconColour": "blue"
  },
  "todo-tree.general.tags": ["BUG", "NOTE", "FIXME", "TODO"],
  "todo-tree.highlights.customHighlight": {
    "NOTE": {
      "icon": "info",
      "type": "line",
      "iconColour": "aquamarine",
      "foreground": "black"
    },
    "BUG": {
      "icon": "bug",
      "type": "line",
      "iconColour": "tomato"
    },
    "TODO": {
      "icon": "check",
      "type": "line",
      "iconColour": "lime",
      "foreground": "DodgerBlue"
    },
    "FIXME": {
      "foreground": "black",
      "iconColour": "yellow"
    }
  }
}
