{
	"Print to console": {
		"prefix": "log",
		"body": [
			"console.log('$1');", "$2"
		],
		"description": "Log output to console"
	},
	"React Component": {
		"prefix": "?react-class",
		"body": ["import React, { Component } from 'react';\nclass ${1:name} extends Component {\n\trender( ) {\n\t\treturn (\n\t\t\t<div>$2</div>\n\t\t)\n\t}\n}\nexport default ${1:name};"],
		"description": "stateful component"
	},
	"Export Const": {
		"prefix": "?export-const",
		"body": ["export const ${1:name} = {\n\t$2\n}"],
		"description": "export const"
	},
	"Import Module": {
		"prefix": "?import-o",
		"body": ["import { $2 } from '$1'"],
		"description": "import object"
	},
	"Get String Static": {
		"prefix": "?strs",
		"body": ["window.str.s."],
		"description": "Get String Static"
	},
	"Get String Dynamic": {
		"prefix": "?strd",
		"body": ["window.str.d."],
		"description": "Get String Dynamic"
	}
}